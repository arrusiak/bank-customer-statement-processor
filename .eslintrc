{
    "extends": "airbnb-base",
    "root": true,
    "rules": {
        "import/prefer-default-export": 0,
        "object-curly-spacing": [
            "error",
            "never"
        ],
        "strict": 0,
        "no-underscore-dangle": 0,
        "curly": 0,
        "linebreak-style": 0,
        "no-multi-spaces": 0,
        "import/no-cycle": 0,
        "key-spacing": 0,
        "no-return-assign": 0,
        "no-restricted-syntax": 0,
        "max-len": 0,
        "no-await-in-loop": 0,
        "consistent-return": 0,
        "no-shadow": 0,
        "comma-dangle": 0,
        "semi": 1,
        "no-use-before-define": 0,
        "no-empty": 0,
        "newline-per-chained-call": 2,
        "new-parens": 0,
        "no-param-reassign": 0,
        "no-cond-assign": 0,
        "no-useless-escape": 0,
        "import/named": 0,
        "no-fallthrough": 0,
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 2
            }
        ],
        "new-cap": 0,
        "no-loop-func": 0,
        "no-unreachable": 0,
        "no-process-exit": 0,
        "quotes": [
            1,
            "single",
            "avoid-escape"
        ]
    },
    "env": {
        "node": true,
        "es6": true
    }
}
